{
    "name": "GoogleMeet attendance",
    "description": "Extension for GoogleMeet attendance. Create by Quốc Khánh, scripts thanks to Hoàng vũ",
    "version": "1.0",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["*://meet.google.com/*"],
        "js": ["scripts/content.js"]
      }
    ],
    "manifest_version": 2
  }